{
    "name": "change background",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "change background--SADON_jung",
    "browser_action": {
        "default_title": "check",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery-3.6.0.min.js","changebg.js"]
        }
    ],
    "permissions" : ["tabs", "activeTab"] 
}